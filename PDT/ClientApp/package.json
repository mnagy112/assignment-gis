{
  "name": "pdt",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "font-awesome": "4.7.0",
    "history": "^4.7.2",
    "leaflet": "^1.3.4",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-leaflet": "^2.1.0",
    "react-loadable": "^5.5.0",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.0",
    "react-toastify": "^4.3.2",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "object-hash": "latest"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --output-style compressed",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --output-style compressed",
    "start-js": "react-scripts start ",
    "start": "rimraf ./build && npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build ",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
